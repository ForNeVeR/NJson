using Nemerle;
using Nemerle.Collections;
using Nemerle.Text;
using Nemerle.Utility;

using System;
using System.Collections.Generic;
using System.Linq;

namespace NJson
{
  /// <summary>
  /// Description of JsonValue.
  /// </summary>
  variant JsonValue
  {
      | String { Value : string; }
      | List { Values : List[JsonValue]; }
      | Object { Values : Hashtable[string, JsonValue]; }
  }
}
